#include <stdio.h>     // standard input output header file

int main()
{
    printf("name\t\t\t\t\t\t\t\t\t\t\t");
    printf("kamaljeet yadav\n");
    printf("Reg.No :\t\t\t\t\t\t\t\t\t\t\t\t\t");
    printf("12214612\n");
    printf("CGPA :\t\t\t\t\t\t\t\t\t\t\t\t\t\t");
    printf("9.3\n");
    printf("Roll :\t\t\t\t\t\t\t\t\t\t\t\t\t\t");
    printf("36");

    int x=10;
    int y=20;
    printf("value of x is=%d\n",x);     //%d is for integer and %f is for float.(percentage/format specifier string );
    printf("value of y is =%d",y);

    float a= 10.43;
    float b= 30.76;
    printf("%f%f=%f",a,b,a+b);
    return "task done";
    
// purpose for return statement : when main function complete his task it give the return statement to the compiler to show that all the task has completed successfully.
//control transfer to main function
    int p;
    float q;
    printf("Enter the data regarding to p and q :::\n");
    scanf("%d",&p);
    scanf("%d",&q);
    printf("value you entered for p is :%d\n",p);
    printf("value you entered for q is :%f",q);

    int x;
    float y_1;
    printf("enter the data here:::::");
    scanf("%d%f",&x,&y_1);
    int z=x+y_1;
    printf("%d",z);

}
